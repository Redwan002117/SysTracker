{
  "name": "agent",
  "version": "3.3.1",
  "description": "SysTracker System Monitoring Agent",
  "bin": "client_agent.js",
  "scripts": {
    "build:win": "pkg . --targets node18-win-x64 --output dist/systracker-agent-win.exe && node scripts/add-icon.js"
  },
  "pkg": {
    "scripts": "client_agent.js",
    "assets": [
      "node_modules/**/*",
      "app.ico"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "license": "SEE LICENSE IN LICENSE",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.13.5",
    "socket.io-client": "^4.8.3",
    "systeminformation": "^5.31.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1",
    "rcedit": "^1.1.1"
  }
}